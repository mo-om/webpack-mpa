		"babel-preset-env": "^1.6.1",
		"babel-preset-stage-2": "^6.24.1",
		"browser-sync": "^2.23.5",
		"browser-sync-webpack-plugin": "^1.2.0",
		"clean-webpack-plugin": "^0.1.17",
		"css-loader": "^0.28.8",
		"cssnano": "^3.10.0",
		"extract-text-webpack-plugin": "^3.0.2",
		"file-loader": "^1.1.6",
